<table>
	<tr id="black">
		<th>Instructor ID</th>
		<th>Instructor Name</th>
		<th>Instructor Office Number</th>
		<th>Instructor Email</th>
	</tr>
</table>
<div class="scroll">
<table class="table table-striped">
<%for instructor in @instructors do%>
	<tr class="<%=cycle('even', 'odd') -%>">
		<td><%=instructor.id%></td>
    	<td><%=instructor.name%></td>
    	<td><%=instructor.office_number%></td>
    	<td><%=instructor.email%>
    		<%=link_to 'Update','#', :class=> 'btn btn-mini'%>
    	</td>
    </tr>
<%end%>
</table>
</div>
